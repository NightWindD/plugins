          "form": true
        },
        "scrapeInterval": {
          "type": "string",
          "title": "监控采集间隔，默认30秒",
          "default": "30s",
          "form": true
        }
      },
      "form": true
    }
  },
  "$schema": "http://json-schema.org/schema#"
